using System;
namespace Sukanta
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            for (int i = 0; i < n; i++)
            {
                int num = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine((num / 2) + 1);
            }
        }
    }
}
